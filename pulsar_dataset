    {
      "cell_type": "code",
      "source": [
        "def GD(X, X_, u, l, weights=None):\n",
        "  Y_gd=np.random.randint(0,2, u)\n",
        "  max_iter=20\n",
        "  k_gd=1\n",
        "  epsilon=1e-2\n",
        "  eta= 1e-4\n",
        "  prev=Y_gd\n",
        "  diff=np.linalg.norm(prev- 1)\n",
        "  delta_loss_limit=1e-3\n",
        "  delta_loss_percent=5e-1\n",
        "  delta_loss=10e3\n",
        "  dloss_frac=1\n",
        "  if weights==None:\n",
        "    print('Initialization of the weights w.r.t. X_ and X.')    \n",
        "    W=initialize_weights(X_, X)\n",
        "    W_=initialize_weights(X, X)\n",
        "    print(f'Initilization done.\\nWeights shape: {W.shape}\\t{W_.shape}')\n",
        "  else:\n",
        "    W=weights[0]\n",
        "    W_=weights[1]\n",
        "  losses_gd=[]\n",
        "  dlosses_gd=[]\n",
        "  acc_gd=[]\n",
        "  while k_gd<max_iter and epsilon < diff and delta_loss > delta_loss_limit and dloss_frac > delta_loss_percent:\n",
        "    #track prev infos\n",
        "    prev=Y_gd\n",
        "    prev_loss=loss(W, W_, prev)\n",
        "    #update\n",
        "    Y_gd = Y_gd- eta*gradient(W, W_, prev)\n",
        "    #check the improvement\n",
        "    diff= np.linalg.norm(Y_gd-prev)\n",
        "    err=loss(W, W_, Y_gd)\n",
        "    delta_loss=np.linalg.norm(err-prev_loss)\n",
        "    dlosses_gd.append(delta_loss)\n",
        "    if k_gd>2:\n",
        "      dloss_frac= (abs(dlosses_gd[k_gd-1]-dlosses_gd[k_gd-2]))/ abs((dlosses_gd[k_gd-2]-dlosses_gd[k_gd-3]))\n",
        "    print(f'Iteration: {k_gd}\\nLoss: {err}\\nPrecision: {diff}')\n",
        "    losses_gd.append(err)\n",
        "    a=accuracy_score(Y, threshold(prev))\n",
        "    acc_gd.append(a)\n",
        "    #next it\n",
        "    k_gd+=1\n",
        "  print(f'Accuracy: {accuracy_score(Y, threshold(Y_gd))}\\nLoss: {losses_gd[len(losses_gd)-1]}')\n",
        "  plt.figure(figsize=(8,5))\n",
        "  plt.plot(range(k_gd-1), losses_gd, label='loss', color='magenta')\n",
        "  plt.grid(linestyle = '--', linewidth = 0.5)\n",
        "  plt.title('Loss w.r.t. iterations') \n",
        "  plt.xlabel(\"Iterations\")\n",
        "  plt.xlim([0, k_gd-2])\n",
        "  plt.ylim([0, losses_gd[0]])\n",
        "  plt.show()\n",
        "  return W, W_, k_gd, losses_gd, acc_gd, Y_gd"
      ],
      "metadata": {
        "id": "SwiHjv2TKoAQ"
      },
      "execution_count": null,
      "outputs": []
    },
