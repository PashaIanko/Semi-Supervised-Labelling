      "source": [
        "#**BCGD with permuted cycle**"
      ],
      "metadata": {
        "id": "PmjrZF5pO-_w"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def BCGD_perm(X, X_, u, l, weights=None):\n",
        "  Y_bcp=np.random.randint(0,2, n-l) #Choose random point in the space R^Y\n",
        "  max_iter=20\n",
        "  k_bcp=1\n",
        "  epsilon=1e-4\n",
        "  eta= 1e-4\n",
        "  prev=Y_bcp\n",
        "  diff=np.linalg.norm(prev- 1)\n",
        "  delta_loss_limit=1e-3\n",
        "  delta_loss_percent=5e-1\n",
        "  delta_loss=10e3\n",
        "  dloss_frac=1\n",
        "  if weights is None:\n",
        "    print('Initialization of the weights w.r.t. X_ and X.')\n",
        "    W=initialize_weights(X_, X)\n",
        "    W_=initialize_weights(X, X)\n",
        "    print(f'Initilization done.\\nWeights shape: {W.shape}\\t{W_.shape}')\n",
        "  else:\n",
        "    W=weights[0]\n",
        "    W_=weights[1]\n",
        "  losses_bcp=[]\n",
        "  dlosses_bcp=[]\n",
        "  acc_bcp=[]\n",
        "  b=n-l\n",
        "\n",
        "  while k_bcp<max_iter and epsilon < diff and delta_loss > delta_loss_limit and dloss_frac > delta_loss_percent:\n",
        "    #track prev infos\n",
        "    Z=Y_bcp\n",
        "    prev=Y_bcp\n",
        "    prev_loss=loss(W, W_, Y_bcp)\n",
        "    #update\n",
        "    cycle=np.random.permutation(b)\n",
        "    for elm in cycle:\n",
        "        prev_j=Z\n",
        "        U_j=col_j(elm)      \n",
        "        Z = Z - eta* U_j * gradient_j(W, W_, prev, elm)\n",
        "    Y_bcp=Z\n",
        "    #check improv\n",
        "    diff= np.linalg.norm(Y_bcp-prev)    \n",
        "    err=loss(W, W_, Y_bcp)\n",
        "    delta_loss=np.linalg.norm(err-prev_loss)\n",
        "    dlosses_bcp.append(delta_loss)\n",
        "    if k_bcp>2:\n",
        "        dloss_frac= (abs(dlosses_bcp[k_bcp-1]-dlosses_bcp[k_bcp-2]))/ abs((dlosses_bcp[k_bcp-2]-dlosses_bcp[k_bcp-3]))\n",
        "    print(f'Iteration: {k_bcp}\\nLoss: {err}\\nPrecision: {diff}')\n",
        "    #next it\n",
        "    losses_bcp.append(err)\n",
        "    a=accuracy_score(Y, threshold(prev))\n",
        "    acc_bcp.append(a)\n",
        "    k_bcp+=1\n",
        "  print(f'Accuracy: {accuracy_score(Y, threshold(Y_bcp))}\\nLoss: {losses_bcp[len(losses_bcp)-1]}')\n",
        "  plt.figure(figsize=(8,5))\n",
        "  plt.plot(range(k_bcp-1), losses_bcp, label='loss', color='blue')\n",
        "  plt.grid(linestyle = '--', linewidth = 0.5)\n",
        "  plt.title('Loss w.r.t. iterations') \n",
        "  plt.xlabel(\"Iterations\")\n",
        "  plt.xlim([0, k_bcp-2])\n",
        "  plt.ylim([0, losses_bcp[0]])\n",
        "  plt.show()\n",
        "  return W, W_, k_bcp, losses_bcp, acc_bcp, Y_bcp"
      ],
      "metadata": {
        "id": "Pt878SbuA-ED"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "Y_bcp=np.random.randint(0,2, n-l) #Choose random point in the space R^Y\n",
        "max_iter=20\n",
        "k_bcp=1\n",
        "epsilon=1e-4\n",
        "eta= 1e-3\n",
        "prev=Y_bcp\n",
        "diff=np.linalg.norm(prev- 1)\n",
        "delta_loss_limit=1e-3\n",
        "delta_loss_percent=5e-1\n",
        "delta_loss=10e3\n",
        "dloss_frac=1\n",
        "#W=initialize_weights(X_, X)\n",
        "#W_=initialize_weights(X, X)\n",
        "losses_bcp=[]\n",
        "dlosses_bcp=[]\n",
        "acc_bcp=[]"
      ],
      "metadata": {
        "id": "kP_MtoO5JUny"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "%%time\n",
        "b=n-l\n",
        "\n",
        "while k_bcp<max_iter and epsilon < diff and delta_loss > delta_loss_limit and dloss_frac > delta_loss_percent:\n",
        "  #track prev infos\n",
        "  Z=Y_bcp\n",
        "  prev=Y_bcp\n",
        "  prev_loss=loss(W, W_, Y_bcp)\n",
        "  #update\n",
        "  cycle=np.random.permutation(b)\n",
        "  for elm in cycle:\n",
        "    prev_j=Z\n",
        "    U_j=col_j(elm)    \n",
        "    Z = Z - eta* U_j * gradient_j(W, W_, prev, elm)\n",
        "  Y_bcp=Z\n",
        "  #check improv\n",
        "  diff= np.linalg.norm(Y_bcp-prev)  \n",
        "  err=loss(W, W_, Y_bcp)\n",
        "  delta_loss=np.linalg.norm(err-prev_loss)\n",
        "  dlosses_bcp.append(delta_loss)\n",
        "  if k_bcp>2:\n",
        "    dloss_frac= (abs(dlosses_bcp[k_bcp-1]-dlosses_bcp[k_bcp-2]))/ abs((dlosses_bcp[k_bcp-2]-dlosses_bcp[k_bcp-3]))\n",
        "  print(f'Iteration: {k_bcp}\\nLoss: {err}\\nPrecision: {diff}')\n",
        "  #next it\n",
        "  losses_bcp.append(err)\n",
        "  a=accuracy_score(Y, threshold(prev))\n",
        "  acc_bcp.append(a)\n",
        "  k_bcp+=1"
      ],
