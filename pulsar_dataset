    {
      "cell_type": "markdown",
      "source": [
        "#**BCGD with Randomized Rule**\n",
        "\n",
        "#*Block Minimization Scheme*"
      ],
      "metadata": {
        "id": "LoRFILiI3djF"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def col_j(j):\n",
        "  U_j=np.zeros((n-l, ))\n",
        "  U_j[j]=1\n",
        "  return U_j"
      ],
      "metadata": {
        "id": "jg2j5xen8VSa"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def BCGD_rand(X, X_, u, l, weights=None):\n",
        "  Y_bcr=np.random.randint(0,2, n-l) #Choose random point in the space R^Y\n",
        "  max_iter=30\n",
        "  k_bcr=1\n",
        "  epsilon=0.0005\n",
        "  eta= 0.0001\n",
        "  prev=Y_bcr\n",
        "  diff=np.linalg.norm(prev- 1)\n",
        "  prev_loss=0\n",
        "  delta_loss_limit=0\n",
        "  delta_loss_percent=0.01\n",
        "  delta_loss=10e3\n",
        "  dloss_frac=1\n",
        "  if weights is None:\n",
        "    print('Initialization of the weights w.r.t. X_ and X.')    \n",
        "    W=initialize_weights(X_, X)\n",
        "    W_=initialize_weights(X, X)\n",
        "    print(f'Initilization done.\\nWeights shape: {W.shape}\\t{W_.shape}')\n",
        "  else:\n",
        "    W=weights[0]\n",
        "    W_=weights[1]\n",
        "  losses_bcr=[]\n",
        "  dlosses_bcr=[]\n",
        "  acc_bcr=[]\n",
        "  b=n-l\n",
        "\n",
        "  while k_bcr<max_iter and epsilon < diff and delta_loss >= delta_loss_limit and dloss_frac > delta_loss_percent:\n",
        "    prev=Y_bcr\n",
        "    j=int(np.random.uniform(0, b, 1))\n",
        "    U_j=col_j(j)        \n",
        "    Y_bcr = Y_bcr - eta* U_j * gradient_j(W, W_, prev, j)\n",
        "    diff= np.linalg.norm(Y_bcr-prev)    \n",
        "    err=loss(W, W_, Y_bcr)\n",
        "    delta_loss=np.linalg.norm(err-prev_loss)\n",
        "    dlosses_bcr.append(delta_loss)\n",
        "    a=accuracy_score(Y, threshold(prev))\n",
        "    if k_bcr>2:\n",
        "        dloss_frac= (abs(dlosses_bcr[k_bcr-1]-dlosses_bcr[k_bcr-2]))/ abs((dlosses_bcr[k_bcr-2]-dlosses_bcr[k_bcr-3]))\n",
        "    #print(err, prev_loss)\n",
        "    print(f'Iteration: {k_bcr}\\nLoss: {err}\\nPrecision: {diff}\\nDelta Loss: {delta_loss}')\n",
        "    losses_bcr.append(err)\n",
        "    acc_bcr.append(a)\n",
        "    k_bcr+=1  \n",
        "  print(f'Accuracy: {accuracy_score(Y, threshold(Y_bcr))}\\nLoss: {losses_bcr[len(losses_bcr)-1]}')\n",
        "  plt.figure(figsize=(8,5))\n",
        "  plt.plot(range(k_bcr-1), losses_bcr, label='loss', color='green')\n",
        "  plt.grid(linestyle = '--', linewidth = 0.5)\n",
        "  plt.title('Loss w.r.t. iterations') \n",
        "  plt.xlabel(\"Iterations\")\n",
        "  plt.xlim([0, k_bcr-1])\n",
        "  plt.show()\n",
        "  return W, W_, k_bcr, losses_bcr, acc_bcr, Y_bcr"
      ],
      "metadata": {
        "id": "D0GWDWEEBJh3"
