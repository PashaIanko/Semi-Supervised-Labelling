      "cell_type": "markdown",
      "source": [
        "#**BCGD with cyclic rule**\n",
        "\n",
        "Everything works as the previous algorithm for the expection that at each iteration we update component by component the gradient. "
      ],
      "metadata": {
        "id": "8O1-ve6zNXyD"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def BCGD_cyclic(X, X_, u, l, weights=None):\n",
        "  Y_bcc=np.random.randint(0,2, n-l) #Choose random point in the space R^Y\n",
        "  max_iter=20\n",
        "  k_bcc=1\n",
        "  epsilon=1e-4\n",
        "  eta= 1e-4\n",
        "  prev=Y_bcc\n",
        "  diff=np.linalg.norm(prev- 1)\n",
        "  delta_loss_limit=1e-3\n",
        "  delta_loss_percent=5e-1\n",
        "  delta_loss=10e3\n",
        "  dloss_frac=1\n",
        "  if weights is None:\n",
        "    print('Initialization of the weights w.r.t. X_ and X.')    \n",
        "    W=initialize_weights(X_, X)\n",
        "    W_=initialize_weights(X, X)\n",
        "    print(f'Initilization done.\\nWeights shape: {W.shape}\\t{W_.shape}')\n",
        "  else:\n",
        "    W=weights[0]\n",
        "    W_=weights[1]\n",
        "  dlosses_bcc=[]\n",
        "  losses_bcc=[]\n",
        "  acc_bcc=[]\n",
        "  b=n-l\n",
        "  while k_bcc<max_iter and epsilon < diff and delta_loss > delta_loss_limit and dloss_frac > delta_loss_percent:\n",
        "    #track previous infos\n",
        "    Z=Y_bcc\n",
        "    prev=Y_bcc\n",
        "    prev_loss=loss(W, W_, Y_bcc)\n",
        "    #update\n",
        "    for j in range(b):\n",
        "      prev_j=Z\n",
        "      U_j=col_j(j)    \n",
        "      Z = Z - eta* U_j * gradient_j(W, W_, prev, j)\n",
        "    Y_bcc=Z\n",
        "    #check improv\n",
        "    diff= np.linalg.norm(Y_bcc-prev)  \n",
        "    err=loss(W, W_, Y_bcc)\n",
        "    delta_loss=np.linalg.norm(err-prev_loss)\n",
        "    dlosses_bcc.append(delta_loss)\n",
        "    if k_bcc>2:\n",
        "      dloss_frac= (abs(dlosses_bcc[k_bcc-1]-dlosses_bcc[k_bcc-2]))/ abs((dlosses_bcc[k_bcc-2]-dlosses_bcc[k_bcc-3]))\n",
        "    print(f'Iteration: {k_bcc}\\nLoss: {err}\\nPrecision: {diff}')\n",
        "    #next it\n",
        "    losses_bcc.append(err)\n",
        "    a=accuracy_score(Y, threshold(prev))\n",
        "    acc_bcc.append(a)\n",
        "    k_bcc+=1\n",
        "  print(f'Accuracy: {accuracy_score(Y, threshold(Y_bcc))}\\nLoss: {losses_bcc[len(losses_bcc)-1]}')\n",
        "  plt.figure(figsize=(8,5))\n",
        "  plt.plot(range(k_bcc-1), losses_bcc, label='loss', color='red')\n",
        "  plt.grid(linestyle = '--', linewidth = 0.5)\n",
        "  plt.title('Loss w.r.t. iterations') \n",
        "  plt.xlabel(\"Iterations\")\n",
        "  plt.xlim([0, k_bcc-2])\n",
        "  plt.ylim([0, losses_bcc[0]])\n",
        "  plt.show()\n",
        "  return W, W_, k_bcc, losses_bcc, acc_bcc, Y_bcc"
      ],
      "metadata": {
        "id": "A0uQvhpSgUs2"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "Y_bcc=np.random.randint(0,2, n-l) #Choose random point in the space R^Y\n",
        "max_iter=20\n",
        "k_bcc=1\n",
        "epsilon=1e-4\n",
        "eta= 1e-3\n",
        "prev=Y_bcc\n",
        "diff=np.linalg.norm(prev- 1)\n",
        "delta_loss_limit=1e-3\n",
        "delta_loss_percent=5e-1\n",
        "delta_loss=10e3\n",
        "dloss_frac=1\n",
        "#W=initialize_weights(X_, X)\n",
        "#W_=initialize_weights(X, X)\n",
        "dlosses_bcc=[]\n",
        "losses_bcc=[]\n",
        "acc_bcc=[]"
      ],
      "metadata": {
        "id": "-3TkUTm2HorZ"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "%%time\n",
        "b=n-l\n",
        "\n",
        "while k_bcc<max_iter and epsilon < diff and delta_loss > delta_loss_limit and dloss_frac > delta_loss_percent:\n",
        "  #track previous infos\n",
        "  Z=Y_bcc\n",
        "  prev=Y_bcc\n",
        "  prev_loss=loss(W, W_, Y_bcc)\n",
        "  #update\n",
        "  for j in range(b):\n",
        "    prev_j=Z\n",
        "    U_j=col_j(j)    \n",
        "    Z = Z - eta* U_j * gradient_j(W, W_, prev, j)\n",
        "  Y_bcc=Z\n",
        "  #check improv\n",
        "  diff= np.linalg.norm(Y_bcc-prev)  \n",
        "  err=loss(W, W_, Y_bcc)\n",
        "  delta_loss=np.linalg.norm(err-prev_loss)\n",
        "  dlosses_bcc.append(delta_loss)\n",
        "  if k_bcc>2:\n",
        "    dloss_frac= (abs(dlosses_bcc[k_bcc-1]-dlosses_bcc[k_bcc-2]))/ abs((dlosses_bcc[k_bcc-2]-dlosses_bcc[k_bcc-3]))\n",
        "  print(f'Iteration: {k_bcc}\\nLoss: {err}\\nPrecision: {diff}')\n",
        "  #next it\n",
        "  losses_bcc.append(err)\n",
        "  a=accuracy_score(Y, threshold(prev))\n",
        "  acc_bcc.append(a)\n",
        "  k_bcc+=1\n",
        "  print(f'Accuracy: {}\\nLoss: {losses_bcc[len(losses_bcc)-1]}')"
      ],
